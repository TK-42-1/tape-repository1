<h2>Full Tape Inventory</h2>
<h4><%= link_to 'Enter New Tape', new_tape_path %></h4>
<table>
<% @tapes.each do |tape| %>
  <tr>
    <td><%= link_to "#{tape.number}", tape %></td>
		<td><%= "#{tape.description}" %></td>
    <td><%= link_to 'Edit', edit_tape_path(tape) %></td>
  </tr>
<% end %>
</table>

<br />